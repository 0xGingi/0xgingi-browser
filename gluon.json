  "appId": "com.0xgingi.browser",
  "binaryName": "0xgingi-browser",
  "version": {
    "product": "firefox",
    "version": "113.0.2"
  },
  "buildOptions": {
    "windowsUseSymbolicLinks": false
  },
  "addons": {
    "ublock": {
      "platform": "github",
      "id": "uBlock0@raymondhill.net",
      "repo": "gorhill/uBlock",
      "version": "1.49.2",
      "fileGlob": "uBlock0_*.firefox(.signed)?.xpi"
    },
    "canvasBlocker": {
      "platform": "github",
      "id": "canvasblocker@kkaspner.de",
      "repo": "kkapsner/CanvasBlocker",
      "version": "1.9",
      "fileGlob": "canvasblocker.xpi"
    },
    "clearUrls": {
      "platform": "github",
      "id": "clearurls@clearurls.xyz",
      "repo": "ClearURLs/Addon",
      "version": "1.23.1",
      "fileGlob": "clearurls-*-firefox.xpi"
    }
  },
  "brands": {
    "gingi": {
      "backgroundColor": "#2b2a33",
      "brandShorterName": "0xGingi",
      "brandShortName": "0xGingi Browser",
